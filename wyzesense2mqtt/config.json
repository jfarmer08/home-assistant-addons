{
  "name": "WyzeSense2MQTT",
  "version": "0.1.1",
  "slug": "wyzesense2mqtt",
  "description": "WyzeSense to MQTT bridge using raetha/wyzesense2mqtt",
  "url": "https://github.com/jfarmer08/home-assistant-addons",
  "arch": ["amd64", "armv7", "aarch64"],
  "startup": "services",
  "boot": "auto",
  "options": {
    "mqtt_host": "core-mosquitto",
    "mqtt_port": 1883,
    "mqtt_username": "",
    "mqtt_password": ""
  },
  "schema": {
    "mqtt_host": "str",
    "mqtt_port": "int",
    "mqtt_username": "str?",
    "mqtt_password": "str?"
  },
  "devices": ["/dev/hidraw0"],
  "udev": true,
  "usb": true,
  "hassio_api": false,
  "hassio_role": "default",
  "host_network": true
}