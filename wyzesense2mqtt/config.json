{
  "name": "WyzeSense2MQTT",
  "version": "0.1.8",
  "slug": "wyzesense2mqtt",
  "description": "Connect WyzeSense to MQTT for use in Home Assistant",
  "url": "https://github.com/raetha/wyzesense2mqtt",
  "startup": "application",
  "boot": "auto",
  "arch": ["amd64", "armhf", "armv7", "aarch64"],
  "host_network": true,
  "usb": true,
  "devices": ["/dev/hidraw0"],
  "map": ["config:rw"],
  "options": {
    "mqtt": {
      "host": "core-mosquitto",
      "port": 1883,
      "topic": "wyzesense"
    }
  },
  "schema": {
    "mqtt": {
      "host": "str",
      "port": "int",
      "topic": "str"
    }
  }
}
